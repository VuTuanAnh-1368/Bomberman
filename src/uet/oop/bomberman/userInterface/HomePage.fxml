<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="365.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uet.oop.bomberman.userInterface.HomePageController">
    <children>
        <ImageView fitHeight="308.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../../../res/image/homeImage.png" />
            </image></ImageView>
        <Button fx:id="playButton" layoutX="72.0" layoutY="313.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="96.0" text="PLAY" textFill="#cab008">
            <font>
                <Font name="Californian FB" size="22.0" />
            </font>
        </Button>
        <Button fx:id="instructionButton" layoutX="339.0" layoutY="313.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="119.0" text="INSTRUCTION" textFill="#c534b6">
            <font>
                <Font name="Californian FB" size="14.0" />
            </font>
        </Button>
        <Button fx:id="exitButton" layoutX="482.0" layoutY="313.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="104.0" text="EXIT" textFill="#bf3f3f">
            <font>
                <Font name="Californian FB" size="15.0" />
            </font>
        </Button>
    </children>
    <effect>
        <DropShadow>
            <color>
                <Color red="0.8868421316146851" green="0.8424999713897705" blue="0.8424999713897705" />
            </color>
        </DropShadow>
    </effect>
</AnchorPane>
